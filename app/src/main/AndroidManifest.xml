        </activity>

        <activity android:name="com.o058team.hajjuserapp.views.ServiceVolunteerLocationActivity" android:screenOrientation="portrait"/>
        
        <meta-data
            android:name="com.google.android.gms.version"
            android:value="@integer/google_play_services_version" />

        <meta-data
            android:name="com.google.android.maps.v2.API_KEY"
